{
  "id": "com.miho.autocamera",
  "name": "Auto Camera Switcher",
  "version": "1.0.1",
  "host": {
    "app": "premierepro",
    "minVersion": "25.0"
  },
  "manifestVersion": 5,
  "requiredPermissions": {
    "localFileSystem": "request",
    "clipboard": "readAndWrite"
  },
  "entryPoints": [
    {
      "id": "starterpanel",
      "type": "panel",
      "main": "index.html",
      "minimumSize": {
        "width": 430,
        "height": 500
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 230,
        "height": 300
      },
      "preferredFloatingSize": {
        "width": 400,
        "height": 300
      },
      "label": {
        "default": "Auto Camera Switcher"
      },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/dark@1x.png",
          "scale": [
            1
          ],
          "theme": [
            "darkest",
            "dark",
            "medium"
          ]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/dark@2x.png",
          "scale": [
            2
          ],
          "theme": [
            "darkest",
            "dark",
            "medium"
          ]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/light@1x.png",
          "scale": [
            1
          ],
          "theme": [
            "lightest",
            "light"
          ]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/light@2x.png",
          "scale": [
            2
          ],
          "theme": [
            "lightest",
            "light"
          ]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 48,
      "height": 48,
      "path": "icons/plugin@1x.png",
      "scale": [
        1
      ],
      "theme": [
        "darkest",
        "dark",
        "medium",
        "lightest",
        "light",
        "all"
      ],
      "species": [
        "pluginList"
      ]
    },
    {
      "width": 48,
      "height": 48,
      "path": "icons/plugin@2x.png",
      "scale": [
        2
      ],
      "theme": [
        "darkest",
        "dark",
        "medium",
        "lightest",
        "light",
        "all"
      ],
      "species": [
        "pluginList"
      ]
    }
  ]
}
